#include <stdio.h>

int main()
{
    char a[100],b[100],c[100];
    int i,j=0;
    printf("Enter the first word ending with $ symbol\n");
    scanf("%s",a);
    printf("Enter the second word ending with $ symbol\n");
    scanf("%s",b);
    for(i=0;a[i]!='\0';i++)
        {
            c[j]=a[i];
            j++;
        }
    for(i=0;b[i]!='\0';i++)
        {
            c[j]=b[i];
            j++;
        }
    printf("The concatenated word is: %s",c);
}
